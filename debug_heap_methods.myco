print("=== Debugging Heap Methods ===");

use heaps as heaps;

# Test heap creation
let heap = heaps.create(True);
print("heap type: " + heap.type.toString());

# Test method access
let isEmpty_method = heap.isEmpty;
print("isEmpty method type: " + isEmpty_method.type.toString());

# Test if the method exists
if isEmpty_method.type == "Function":
    print("isEmpty method found");
else:
    print("isEmpty method not found, type: " + isEmpty_method.type.toString());

print("Done!");
