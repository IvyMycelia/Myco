print("=== Debugging type function ===");

print("Testing type(True):");
let type_result = type(True);
print("type(True) result:", type_result);
print("type(True) == 'Boolean':", type_result == "Boolean");

print("Testing type(42):");
let type_num = type(42);
print("type(42) result:", type_num);

print("Testing type('hello'):");
let type_str = type("hello");
print("type('hello') result:", type_str);
