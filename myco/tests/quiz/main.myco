let score = 0;

for i in 1:10:
    let a = i;
    let b = i + 1;
    print("Question", i, "of 10:");
    print("What is", a, "+", b, "? ");

    let ans = input("Your answer: ");
    if ans == a + b:
        print("Correct!");
        let score = score + 1;
    else:
        print("Incorrect.");
    end
end

print("Final score:", score, "/ 10!");