print("Testing try-catch...");

let error_caught = 0;
print("About to enter try block...");

try:
    print("Inside try block...");
    let bad_division = 10 / 0;
    print("This should not print!");
catch err:
    print("Inside catch block!");
    print("Error:", err);
    error_caught = 1;
end

print("After try-catch block");
print("error_caught =", error_caught);
print("Done.");
