# Test current deep nesting capabilities
let user = {
    name: "Alice",
    profile: {
        email: "alice@example.com",
        address: {
            street: "123 Main St",
            city: "NYC",
            coordinates: {
                lat: 40.7128,
                lng: -74.0060
            }
        }
    }
};

print("User:", user);
print("Level 1 access - name:", user.name);
print("Level 2 access - email:", user.profile.email);
print("Level 3 access - city:", user.profile.address.city);
print("Level 4 access - lat:", user.profile.address.coordinates.lat);

# Test bracket access too
print("Bracket Level 2:", user["profile"]["email"]);
print("Bracket Level 3:", user["profile"]["address"]["city"]);
